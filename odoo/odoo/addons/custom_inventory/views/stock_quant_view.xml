<!-- views/stock_inventory_view.xml -->
<odoo>
  <record id="view_inventory_form_inherit_custom" model="ir.ui.view">
    <field name="name">stock.quant.inventory.tree.editable.inherit.qc</field>
    <field name="model">stock.quant</field>
    <field name="inherit_id" ref="stock.view_stock_quant_tree_inventory_editable"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='inventory_quantity']" position="after">
        <field name="x_qc_passed"/>
      </xpath>
    </field>
  </record>
</odoo>