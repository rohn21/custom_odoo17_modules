<odoo>
    <data noupdate="1"/>
    <record id="ir_cron_action_to_do" model="ir.cron">
        <field name="name">Tested Scheduled Cron</field>
        <field name="model_id" ref="model_custom_cron_model"/>
        <field name="state">code</field>
        <field name="code">model.action_cron_test_method()</field>
        <field name='interval_number'>5</field>
        <field name='interval_type'>minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="active" eval="True"/>
    </record>


    <data noupdate="1">
        <record id="ir_cron_auto_archive_old_sales" model="ir.cron">
            <field name="name">Auto Archive Old Sales</field>
            <field name="model_id" ref="sale.model_sale_order"/>
            <field name="state">code</field>
            <field name="code">model.auto_archive_old_sales()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
        </record>
    </data>

</odoo>